<div class="box" id="view-clientes">
  <div class="m-4">
    <table class="table">
      <thead class="table-dark">
        <tr>
          <th scope="col">#</th>
          <th scope="col">Cliente</th>
          <th scope="col">Contrato</th>
          <th scope="col">Montos</th>
          <th scope="col">Fecha</th>
        </tr>
      </thead>
      <tbody>
        <% contratos.forEach(function(contrato) { %>
          <tr>
            <th scope="row">
              <%=contrato.id%>
            </th>
            <td>
              <%=contrato.cliente.nombre%>
            </td>
            <td>
              <%=contrato.contrato%>
            </td>
            <td>
              <%=contrato.monto%>
            </td>
            <td>
              <%=contrato.fecha%>
            </td>

          </tr>
          <% }); %>
      </tbody>
    </table>
  </div>
  <div>
    <div class="form-inline m-4">
      <form action="/" method="get">
          <input type="date" name="fechaInicial" value="<%=inicio%>"/>
          <input type="date" name="fechaFinal" value="<%=final%>"/>
          <button type="submit" class="btn btn-dark">Buscar</button>
      </form>
     </div>
  </div>
  <div class="m-4">
    <table class="table">
      <thead class="table-dark">
        <tr>
          <th scope="col">Cliente</th>
          <th scope="col">Monto Total</th>
        </tr>
      </thead>
      <tbody>
        <% if(busqueda) { %>
          <% busqueda.forEach(function(contrato) { %>
          <tr>
            <td>
              <%=contrato[0]%>
            </td>
            <td>
              <%=contrato[1]%>
          </tr>
          <% }); %>
        <% } %>
      </tbody>
    </table>
  </div>
</div>